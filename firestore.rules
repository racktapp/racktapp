rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      // Allow read/write access for a limited time for testing.
      // WARNING: Make sure you secure your database before going to production.
      allow read, write: if request.time < timestamp.date(2025, 9, 6);
    }
  }
}
